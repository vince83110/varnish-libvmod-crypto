varnishtest "test vmod-crypto"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
        import crypto;

        sub vcl_deliver {
	    set resp.http.Hello = crypto.hello();
        }
} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 200
	expect resp.http.Hello == "vmod-crypto"
} -run
